{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "eol-last":"off",
        "linebreak-style":"off",
        "indent":"off",
        "no-console":"off",
        "max-len":"off",
        "comma-dangle":"off",
        "no-trailing-spaces":"off"
    }
}
